{
  "extends": "react-app",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "jsx-a11y/anchor-has-content": "off",
    "no-useless-escape": "off",
    "no-eval": "off",
    "quotes": ["error", "single", { "allowTemplateLiterals": true }],
    "no-array-constructor": "error",
    "no-multi-spaces": "error",
    "semi": ["error", "never"],
    "react/prop-types": "warn",
    "no-var": "error"
  }
}
